@* Sets the title tag including conditions to display the correct pharmacy name *@
@if(cpaos == 1119){
  int cpaos3 = CurrentPage.AncestorOrSelf(3).Id;
  if(cpaos3 == 1247){
    <title>@CurrentPage.pharmacyName | @CurrentPage._siteTitle</title>
  } else {
    <title>@CurrentPage.Name | @CurrentPage._siteTitle</title>
  }
} else {
