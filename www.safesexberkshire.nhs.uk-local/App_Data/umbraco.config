    <PanelRepository id="1219" key="45c63d8f-1b7e-4dd1-8405-4692dbbbecaf" parentID="1110" level="2" creatorID="0" sortOrder="14" createDate="2016-03-04T09:50:31" updateDate="2016-03-30T15:55:25" nodeName="Home Page Panels" urlName="home-page-panels" path="-1,1110,1219" isDoc="" nodeType="1217" creatorName="Webteam" writerName="Webteam" writerID="0" template="0" nodeTypeAlias="PanelRepository">
